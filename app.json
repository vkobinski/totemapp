{
  "expo": {
    "name": "Totem",
    "slug": "totem",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kodev.uptotem",
      "buildNumber": "14",
      "infoPlist": {
        "NSAppTransportSecurity": "Esse app usa requisições HTTP.",
        "NSAllowsArbitraryLoads": "Requisições HTTP"
      }
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kodev.uptotem"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 33,
            "targetSdkVersion": 33,
            "buildToolsVersion": "33.0.0"
          },
          "ios": {
            "deploymentTarget": "13.0",
            "useFrameworks": "static"

          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "6fac23d5-f2e5-43b9-902b-5f5bdfaf7f38"
      }
    }
  }
}
